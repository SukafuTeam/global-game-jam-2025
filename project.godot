config_version=5

[application]

config/name="CapyBubble"
config/version="1.0.0"
run/main_scene="res://scenes/logo_scene.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
config/icon="res://icon.png"

[autoload]

SoundController="*res://entities/globals/sound_controller.tscn"
InputController="*res://scripts/globals/input_controller.gd"
GameController="*res://scripts/globals/game_controller.gd"
Constants="*res://entities/globals/constants.tscn"
SceneTransition="*res://entities/globals/scene_transition.tscn"
InputDetector="*res://entities/globals/input_detector.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://assets/": "red",
"res://assets/sound/": "red",
"res://data/": "green",
"res://entities/": "pink",
"res://scenes/": "purple",
"res://scripts/": "blue"
}

